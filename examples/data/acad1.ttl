@prefix brick: <https://brickschema.org/schema/1.0.3/Brick#> .
@prefix bf: <https://brickschema.org/schema/1.0.3/BrickFrame#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix tag: <https://brickschema.org/schema/1.1/BrickTag#> .

@prefix acad1: <http://example.com/building/acad1#>.
@prefix : <http://example.com/building/acad1#>.

tag:Aggregate a brick:Tag ;
    rdfs:label "Aggregate" .

:ACAD1 a brick:Building;
  brick:hasPart :FLG;
  brick:hasPart :FL1;
  brick:hasPart :FL2;
  brick:hasPart :WINGC.

:FLG a brick:Floor;
  # brick:isPartOf :ACAD1;
  brick:hasPart :CG11.
:FL1 a brick:Floor.
  # brick:isPartOf :ACAD1.
:FL2 a brick:Floor;
  # brick:isPartOf :ACAD1;
  brick:hasPart :C213;
  brick:hasPart :C212;
  brick:hasPart :C205;
  brick:hasPart :C201.

:WINGC a brick:Wing;
  # brick:isPartOf :ACAD1;
  brick:hasPart :CG11;
  brick:hasPart :C212;
  brick:hasPart :C213;
  brick:hasPart :C205;
  brick:hasPart :C201.


:C213 a brick:Laboratory.
:C212 a brick:Laboratory.

    # brick:isPartOf :FL2;
    # brick:isPartOf :WINGC.
:C201 a brick:Room.
    # brick:isPartOf :FL2;
    # brick:isPartOf :WINGC.
:C205 a brick:Room.
    # brick:isPartOf :FL2;
    # brick:isPartOf :WINGC.
:CG11 a brick:Server_Room.
    # brick:isPartOf :FLG;
    # brick:isPartOf :WINGC.

# Server room temperature sensors
:CG11_ZN1_T a brick:Zone_Air_Temperature_Sensor.
:CG11_ZN2_T a brick:Zone_Air_Temperature_Sensor.
:CG11_ZN3_T a brick:Zone_Air_Temperature_Sensor.
:CG11_ZN4_T a brick:Zone_Air_Temperature_Sensor.
:CG11_ZN5_T a brick:Zone_Air_Temperature_Sensor.
:CG11_POW_A a brick:Power_Sensor .
:CG11 brick:hasPoint :CG11_ZN1_T.
:CG11 brick:hasPoint :CG11_ZN2_T.
:CG11 brick:hasPoint :CG11_ZN3_T.
:CG11 brick:hasPoint :CG11_ZN4_T.
:CG11 brick:hasPoint :CG11_ZN5_T.
:CG11 brick:hasPoint :CG11_POW_A.


# Laboratory power sensors
:C212_POW_A a brick:Power_Sensor .
:C212_POW_L a brick:Power_Sensor .
:C213_POW_A a brick:Power_Sensor .
:C213_POW_L a brick:Power_Sensor .
:C212 brick:hasPoint :C212_POW_A.
:C212 brick:hasPoint :C212_POW_L.
:C213 brick:hasPoint :C213_POW_A.
:C213 brick:hasPoint :C213_POW_L.

# Laboratory temperature sensors
:C212_ZNT_1 a brick:Zone_Air_Temperature_Sensor .
:C212_ZNT_2 a brick:Zone_Air_Temperature_Sensor .
:C213_ZNT_1 a brick:Zone_Air_Temperature_Sensor .
:C213_ZNT_2 a brick:Zone_Air_Temperature_Sensor .
:C212 brick:hasPoint :C212_ZNT_1.
:C212 brick:hasPoint :C212_ZNT_2.
:C213 brick:hasPoint :C213_ZNT_1.
:C213 brick:hasPoint :C213_ZNT_2.

# Classroom power sensor
:C205_POW_A a brick:Power_Sensor .
:C205 brick:hasPoint :C205_POW_A.
